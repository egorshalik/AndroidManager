@model IEnumerable<ManagerAndroid.Models.Job>

@{
    ViewBag.Title = "Jobs";
}

<table class="table">
    <tr><td>Name</td><td>Description</td><td>Complexity</td></tr>
    @foreach (var p in Model)
    {
        <tr>
            <td>@p.Name</td>
            <td>@p.Description</td>
            <td>@p.Complexity</td>
            <td>
                <a asp-action="Job" asp-controller="Job" asp-route-id="@p.Id">Details</a> |
                <a asp-action="Edit" asp-controller="Job" asp-route-id="@p.Id">Edit</a> |
                <a asp-action="Delete" asp-controller="Job" asp-route-id="@p.Id">Delete</a>
            </td>
        </tr>
    }
</table>